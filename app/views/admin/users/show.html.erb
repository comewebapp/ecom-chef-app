<div class="row">
  <div class="co-md-12">
    <div class="col-md-8">
      <div class="row">
        <div class="col-md-6">Id</div>
        <div class="col-md-6"><%= @user.id %></div>

        <div class="col-md-6">Vendor</div>
        <div class="col-md-6"><%= @user.vendor %></div>

        <div class="col-md-6">First Name</div>
        <div class="col-md-6"><%= @user.name %></div>

        <div class="col-md-6">Last Name</div>
        <div class="col-md-6"><%= @user.lastname %></div>

        <div class="col-md-6">Email</div>
        <div class="col-md-6"><%= @user.email %></div>

        <div class="col-md-6">Telephone</div>
        <div class="col-md-6"><%= @user.contact %></div>

        <div class="col-md-6">Address</div>
        <div class="col-md-6"><%= @user.address %></div>

        <div class="col-md-6">Postal Code</div>
        <div class="col-md-6"><%= @user.postalcode %></div>

        <div class="col-md-6">City</div>
        <div class="col-md-6"><%= @user.city %></div>

        <div class="col-md-6">Bank Account</div>
        <div class="col-md-6"><%= @user.bankaccount %></div>

        <div class="col-md-6">Bank Name</div>
        <div class="col-md-6"><%= @user.bankname %></div>

        <div class="col-md-6">Bank Owner</div>
        <div class="col-md-6"><%= @user.bankowner %></div>

        <div class="col-md-6">Collections</div>
        <div class="col-md-6"><%= @user.collections.map(&:title).join(", ") %></div>
      </div>
    </div>
  </div>
</div>
